   "source": [
    "import cv2\n",
    "\n",
    "# read input image\n",
    "img = cv2.imread('expected.jpg')\n",
    "\n",
    "# resize image\n",
    "resized_img = cv2.resize(img, (500, 500))\n",
    "\n",
    "# save output image in place of input image\n",
    "cv2.imwrite('expected.jpg', resized_img)\n"
   ]
